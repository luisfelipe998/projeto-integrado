meta {
  name: Get Books By Title
  type: http
  seq: 2
}

get {
  url: {{host}}/books?title=Clean Code
  body: none
  auth: basic
}

params:query {
  title: Clean Code
  ~status: available
  ~status: rented
  ~status: late
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

assert {
  res.body.length: eq 1
  res.body[0].title: contains Clean Code
  res.status: eq 200
}
