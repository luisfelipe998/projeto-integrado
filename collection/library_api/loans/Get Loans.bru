meta {
  name: Get Loans
  type: http
  seq: 1
}

get {
  url: {{host}}/loans
  body: none
  auth: basic
}

params:query {
  ~bookid: 4
  ~status: active
  ~status: late
  ~status: returned
}

auth:basic {
  username: {{username}}
  password: {{password}}
}

assert {
  res.body.length: gt 0
  res.status: eq 200
}
